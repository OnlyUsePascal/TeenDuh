<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".view.activity.TestFeature">

  <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
      android:id="@+id/button"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toWelcome"
      android:text="Welcome page x" />

    <Button
      android:id="@+id/button2"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toAuthLayout"
      android:text="authenticate (otp x, gg)" />

    <Button
      android:id="@+id/button3"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toSignUp"
      android:text="Sign up info x" />

    <Button
      android:id="@+id/button6"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toMainLayout"
      android:text="main layout (swipe, chat, profile)" />

    <Button
      android:id="@+id/button4"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Permission" />

    <Button
      android:id="@+id/button9"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:onClick="toPayment"
      android:text="Payment" />
  </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>